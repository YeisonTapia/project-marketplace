  background-size contain, contain
  background-position right center, left center
  background-repeat no-repeat
  & .line
    background $bg-degradado  
    height 15px
    width 100%      

.banner-category
  .title-label
    width auto
    font-size 50px
    padding 15px 35px
    @media screen and (max-width: $breakpoint-md)
      font-size 30px 
      padding 10px     
      
.store-page
  .border-bottom-gray
    border-bottom 1px solid #E1E1E1
  .btn-arrow-send-pink:after
    right 90%      
  .card-border-right
    border-right 3px solid #E1E1E1
    padding-right 32px
    @media screen and (max-width: $breakpoint-md)
        border-right 0
        padding-right 0  
  #treeListSelect
    border 0 !important
  .img-message
    position absolute
    bottom 19%
    width 100%
    padding-right 16px
    text-align center
    font-size 1rem
    opacity .7 
  .ratio-4-3  
    .contain
      object-fit contain
  .img-product        
    .ratio-4-3  
      .contain
        object-fit cover  

.line-text
  position relative    
  .line-grey
    position absolute
    top 50%
    z-index 1
    @media screen and (max-width: $breakpoint-xs)
      display none
  span  
    position relative
    z-index 2     
    
.plan      
  & .img-title
    object-fit contain
    width  auto
    margin-top -40px
    position absolute
    right 20px   
  & .list-plan   
    margin-bottom 10px 
    display flex
  & .q-card__actions
    position absolute
    width 100%
    bottom 0
  & .q-card-title
    height 20px
  & .q-btn-end
    border-radius 0
    box-shadow none  
    margin-bottom -8px           

.stairs
  .step
    margin-left -10px
  & .step:nth-child(odd)
    z-index 9
    position relative
    margin-bottom -0px
  & .step:nth-child(even)
    z-index 8
    position relative
    margin-bottom -20px
  @media screen and (max-width: $breakpoint-sm)
    .step
      margin-left 0 !important    

.q-icon-round    
  border-radius 50%
  border 1px solid $primary
  padding 10px
  margin-right 15px
  width 60px
  height 60px   
  
